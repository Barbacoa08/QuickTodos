{
	"name": "todo-example",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"all": "pnpm i && pnpm build && pnpm check && pnpm test",
		"github": "pnpm i && pnpm build && pnpm check && pnpm test",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "3.3.1",
		"@sveltejs/kit": "2.7.4",
		"@sveltejs/vite-plugin-svelte": "4.0.0",
		"svelte": "5.1.9",
		"svelte-check": "4.0.5",
		"typescript": "5.6.3",
		"vite": "5.4.10",
		"vite-plugin-pwa": "0.20.5",
		"vitest": "2.1.4"
	}
}
